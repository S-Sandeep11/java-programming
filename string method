class Main {
    public static void main(String[] args) {
 String a = "New World";
 String y = "Hello World";
 String x ="Hello|World|java";
 
 System.out.println("Hello".matches(x));
 System.out.println("World".matches(x));
 System.out.println("javascript".matches(x));
 System.out.println("y".regionMatches(7,a,4,5));
    }
}





class Main {
    public static void main(String[] args) {
 String x = "Hello";
 String y ="Hello sandeep Hello";
 System.out.println(x.replace('l','p'));
 String a = "(?i)Hello";
 
 System.out.println(y.replaceAll(a,"hi"));
 String b = "Hello";
 System.out.println(y.replaceFirst(b,"hi"));
    }
}





class Main {
    public static void main(String[] args) {
 String x = "Hello World";
 String y ="[,\\.\\s]";
 String [] z = x.split(y) ;
    for(String S : z) {
 System.out.println(S);
    }
  }
}





class Main {
    public static void main(String[] args) {
 String x = "Hello";
 System.out.println(x.startsWith("Hello"));
 System.out.println(x.startsWith("He"));
    }
}





class Main {
    public static void main(String[] args) {
        String x = "Sandeep";
        char[] y =x.toCharArray();
        System.out.println(y[0]);
    }
}



import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String x = "     sandeep     ";
        System.out.println(x.trim());
        System.out.println(String.valueOf(x));
         System.out.println(String.valueOf(true));
    }
}
        


import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data1 = "Hello";
        String data2 = "Hello";
        int mystr = data1.codePointAt(0);
        int result = data1.compareTo(data2);
        System.out.print("Enter a character: ");
        char y = sc.next().charAt(0);
        int sec = data1.codePointAt(0);
        System.out.println("Code point at 0: " + mystr);
        System.out.println("Comparison result: " + result);
        System.out.println("You entered: " + y);
        System.out.println("Second code point (same as first here): " + sec);
        sc.close();
    }
}



import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data1 = "Hello";
        String data2 = "Helloi";
        int mystr = data1.codePointAt(0);
        int result = data1.compareTo(data2);
        System.out.print("Enter a character: ");
        char y = sc.next().charAt(0);
        int sec = data1.codePointAt(0);
        System.out.println(mystr);
        System.out.println(result);
        System.out.println(y);
        System.out.println(sec);
        sc.close();
    }
}





public class Main {
    public static void main(String[] args) {
        String data = "hello";
        System.out.println(data.contentEquals("hello"));
        System.out.println(data.contentEquals("e"));
        System.out.println(data.contains("hell"));
        System.out.println(data.contentEquals("h"));
        System.out.println(data.contentEquals("hel"));
    }
}


