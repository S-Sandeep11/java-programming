import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data1 = "Hello";
        String data2 = "Hello";
        int mystr = data1.codePointAt(0);
        int result = data1.compareTo(data2);
        System.out.print("Enter a character: ");
        char y = sc.next().charAt(0);
        int sec = data1.codePointAt(0);
        System.out.println("Code point at 0: " + mystr);
        System.out.println("Comparison result: " + result);
        System.out.println("You entered: " + y);
        System.out.println("Second code point (same as first here): " + sec);
        sc.close();
    }
}


import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data1 = "Hello";
        String data2 = "Helloi";
        int mystr = data1.codePointAt(0);
        int result = data1.compareTo(data2);
        System.out.print("Enter a character: ");
        char y = sc.next().charAt(0);
        int sec = data1.codePointAt(0);
        System.out.println(mystr);
        System.out.println(result);
        System.out.println(y);
        System.out.println(sec);
        sc.close();
    }
}


import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data = "Hello";
        System.out.println(data.contains("ll"));
        System.out.println(data.equals("Hello"));
        System.out.println(data.equalsIgnoreCase("hello"));
        char[] chars = {'H', 'e', 'l', 'l', 'o'};
        System.out.println(String.copyValueOf(chars));
        System.out.println(String.valueOf(chars));
        sc.close();
    }
}


public class Main{
    public static void main(String[] args) {
      String name="Kathir";
      byte[] arr=name.getBytes();
      System.out.println(arr[0]);
       
    }
}


public class Main{
    public static void main(String[] args) {
        char[] data={'0','1','2','3','4','5','6','7','8'};
      System.out.println(data);
      String mystring="HELLO WORLD";
      mystring.getChars(6,11,data,4);
      System.out.println(data);
    }
}


import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String data1 = "Hello";
        String data2 = "Hello";
        int mystr = data1.codePointAt(0);
        int result = data1.compareTo(data2);
        System.out.print("Enter a character: ");
        char y = sc.next().charAt(0);
        int sec = data1.codePointAt(0);
        System.out.println("Code point at 0: " + mystr);
        System.out.println("Comparison result: " + result);
        System.out.println("You entered: " + y);
        System.out.println("Second code point (same as first here): " + sec);
        sc.close();
    }
}


