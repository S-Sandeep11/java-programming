import java.util.Scanner;
public class forloopExample {
    public static void main(String[] args) {
        for (int i = 1; i<=10;i++)
        {
          if(i==5){
              continue;
          }
            System.out.println("value of i:" + i);
            if(i==5) {
                break;
            }
          }
        }
    }
